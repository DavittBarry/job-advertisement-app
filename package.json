      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.test.js": "eslint --cache --fix",
    "*.vue": "eslint --cache --fix"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
